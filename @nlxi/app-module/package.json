{
  "name": "@nlxi/app-module",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.{j,t}s\"",
    "start": "nest start",
    "start:dev": "NODE_OPTIONS='--experimental-specifier-resolution=node' nest start --watch",
    "start:debug": "NODE_OPTIONS='--experimental-specifier-resolution=node' nest start --debug --watch",
    "start:prod": "NODE_OPTIONS='--experimental-specifier-resolution=node' node build/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{j,t}s\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "./typeorm.sh"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.4",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/ioredis": "^5.0.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "ioredis": "^5.2.4",
    "module-alias": "^2.2.2",
    "nanoid": "^4.0.0",
    "nestjs-pino": "^3.1.1",
    "pg": "^8.8.0",
    "pino": "^8.7.0",
    "pino-http": "^8.2.1",
    "pino-loki": "^2.0.3",
    "pino-pretty": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "preset": "ts-jest/presets/default-esm",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "imports": {
    "#root/*": "./build/*"
  }
}
